if msg.datasize == 0:
    datasize = contract.storage[0]
    data = array(datasize)
    while i < datasize:
        data[i] = contract.storage[i + 1]
        i += 1
    return(data, datasize)
else:
    contract.storage[0] = msg.datasize

    i = 0
    while i < msg.datasize:
        contract.storage[i + 1] = msg.data[i]
        i += 1
    return(1)
